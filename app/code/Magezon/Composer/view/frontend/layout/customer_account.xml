<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="composer_accesskeys_my_account">
                <arguments>
                    <argument name="path" xsi:type="string">composer/accesskeys</argument>
                    <argument name="label" xsi:type="string">My Access Keys</argument>
                    <argument name="sortOrder" xsi:type="number">150</argument>
                    <argument name="navigation" xsi:type="boolean">true</argument>
                </arguments>
            </block>
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="composer_packages_my_account">
                <arguments>
                    <argument name="path" xsi:type="string">composer/packages</argument>
                    <argument name="label" xsi:type="string">My Packages</argument>
                    <argument name="sortOrder" xsi:type="number">140</argument>
                    <argument name="navigation" xsi:type="boolean">true</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>